{% extends 'veloshop/layout.html.twig' %}

{% block content %}
    <div class="breadcrumbs">
        {% include 'veloshop/components/breadcrumbs.html.twig' %}
    </div>

    <div class="product.list" >
        {% for row in 0..2 %}
            <div class="row">
                {% for column in 0..4 %}
                    <div class="product" style="max-width: 20%">
                        {% include 'veloshop/components/card.html.twig' %}
                    </div>
                {% endfor %}
            </div>
        {% endfor %}
    </div>
{% endblock %}